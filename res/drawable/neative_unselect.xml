<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <!-- content -->
    <item>
        <shape android:shape="rectangle" >

            <!-- 填充的颜色 -->
            <solid android:color="#FF6347"/>
            <!-- 设置按钮的四个角为弧形 -->
            <!-- android:radius 弧形的半径 -->
            <corners android:radius="5dip" />
            
        </shape>
    </item>

</layer-list>